# proc list to monitor
local mproclist="tmpServer uhttpd dnsmasq client_mgmt cloud_brd switch_led ubusd url_class connindicator"
# "proc_name init_file pid_file"
local tmpServer="tmpServer tmpServer /var/run/tmpServer.pid"
local uhttpd="uhttpd uhttpd /var/run/uhttpd_main.pid"
local dnsmasq="dnsmasq dnsmasq /var/run/dnsmasq.pid"
local client_mgmt="client_mgmt client_mgmt /var/run/client_mgmt.pid"
local cloud_brd="cloud-brd cloud_brd /var/cloud.pid" 
local switch_led="switch_led switch_led /var/run/switch_led"
local ubusd="ubusd ubus /var/run/ubusd.pid"
# cloud_account default state is not bind, so url_class default not
#local url_class="url-class url_class /var/run/url-class.pid"
local connindicator="conn-indicator zzzzzzaconn-indicator /var/run/conn-indicator.pid"
